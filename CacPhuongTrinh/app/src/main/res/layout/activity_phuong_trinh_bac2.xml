<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".PhuongTrinhBac2">


    <TextView
        android:id="@+id/textView7"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginStart="12dp"
        android:layout_marginTop="160dp"
        android:text="Hệ Số A"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView8"
        android:layout_width="118dp"
        android:layout_height="60dp"
        android:layout_marginStart="4dp"
        android:layout_marginTop="232dp"
        android:text="Hệ Số B"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="116dp"
        android:layout_height="57dp"
        android:layout_marginStart="8dp"
        android:layout_marginTop="320dp"
        android:text="Hệ Số C"
        android:textAlignment="center"
        android:textAllCaps="false"
        android:textSize="20sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView10"
        android:layout_width="108dp"
        android:layout_height="51dp"
        android:layout_marginStart="16dp"
        android:layout_marginTop="436dp"
        android:text="Kết Quả"
        android:textAlignment="textStart"
        android:textAllCaps="true"
        android:textSize="20sp"
        android:textStyle="bold|italic"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/txta"
        android:layout_width="249dp"
        android:layout_height="45dp"
        android:layout_marginTop="160dp"
        android:layout_marginEnd="25dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtb"
        android:layout_width="241dp"
        android:layout_height="43dp"
        android:layout_marginTop="249dp"
        android:layout_marginEnd="33dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textAllCaps="false"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtc"
        android:layout_width="235dp"
        android:layout_height="45dp"
        android:layout_marginTop="335dp"
        android:layout_marginEnd="37dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <EditText
        android:id="@+id/txtx"
        android:layout_width="239dp"
        android:layout_height="43dp"
        android:layout_marginTop="437dp"
        android:layout_marginEnd="33dp"
        android:ems="10"
        android:inputType="number"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:textStyle="bold|italic"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:ignore="MissingConstraints" />

    <Button
        android:id="@+id/btngui"
        android:layout_width="102dp"
        android:layout_height="54dp"
        android:layout_marginTop="528dp"
        android:layout_marginEnd="52dp"
        android:text="Gửi"
        app:icon="@android:drawable/presence_online"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:rippleColor="#E80F0F" />

    <TextView
        android:id="@+id/textView11"
        android:layout_width="386dp"
        android:layout_height="61dp"
        android:layout_marginStart="6dp"
        android:layout_marginEnd="19dp"
        android:layout_marginBottom="9dp"
        android:text="Ma Nguyễn Nhật Tân"
        android:textAlignment="center"
        android:textColor="#C82F76"
        android:textSize="24sp"
        android:textStyle="bold|italic"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:ignore="MissingConstraints" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="396dp"
        android:layout_height="90dp"
        android:layout_marginStart="6dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="9dp"
        android:fontFamily="sans-serif-medium"
        android:text="Phương Trình Bậc 2"
        android:textAlignment="center"
        android:textColor="#EA0C0C"
        android:textSize="34sp"
        android:textStyle="bold"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnlamlai"
        android:layout_width="142dp"
        android:layout_height="54dp"
        android:layout_marginStart="36dp"
        android:layout_marginTop="528dp"
        android:text="Làm Lại"
        android:textAlignment="center"
        app:icon="@android:drawable/stat_notify_sync"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnthoat"
        android:layout_width="141dp"
        android:layout_height="56dp"
        android:layout_marginStart="37dp"
        android:layout_marginTop="596dp"
        android:background="#EC0606"
        android:backgroundTint="#DF1616"
        android:backgroundTintMode="add"
        android:text="Thoát"
        android:textColor="#D8E14141"
        android:textStyle="bold"
        app:icon="@android:drawable/ic_delete"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:strokeColor="#EA0909" />

</androidx.constraintlayout.widget.ConstraintLayout>





